{
    "name": "cantie/app-store-connect-api",
    "description": "A PHP client library for accessing App Store Connect APIs",
    "keywords": ["apple", "appstore", "appstore-api", "php"],
    "license": "MIT",
    "authors": [
        {
            "name": "Long Pham",
            "email": "cantie912@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.4|^8.0",
        "google/auth": "^1.28",
        "guzzlehttp/guzzle": "^7.0.1",
        "firebase/php-jwt": "~6.0",
        "monolog/monolog": "^2.9||^3.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Cantie\\AppStoreConnect\\": "src/"
        },
        "files": [
            "src/aliases.php"
        ],
        "classmap": [
            "src/aliases.php"
        ]
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10"
    }
}